public class Main {
    public static void main(String[] args) {
        int levels = 265;
        for(int a=1;a<=levels;a++){
            for(int b=a;b<=levels;b++){
                System.out.print("");
            }

            for(int c=0;c<=(a*1)-1;c++){
                System.out.print("3");
            }

            System.out.println();
        }
    }
}
